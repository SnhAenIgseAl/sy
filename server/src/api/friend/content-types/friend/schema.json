{
  "kind": "collectionType",
  "collectionName": "friends",
  "info": {
    "singularName": "friend",
    "pluralName": "friends",
    "displayName": "Friend"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "friend_name": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
    "desc": {
      "type": "string"
    },
    "friend_status": {
      "type": "enumeration",
      "default": "pending",
      "enum": [
        "pending",
        "finished"
      ]
    },
    "screenshoot": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images"
      ]
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "friends"
    }
  }
}
